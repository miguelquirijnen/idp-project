vocabulary V{
	type Color
	type Area
	Border(Area,Area)
	Coloring(Area):Color
}

theory T : V {
	// Two adjacent countries can not have the same color
	!a1[Area], a2[Area]: Border(a1, a2) => Coloring(a1) ~= Coloring(a2).
    
}

